{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "lib": ["ES2021", "DOM"],
    "jsx": "react-jsx",
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "strict": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["react-native", "node"],
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "outDir": "build"
  },
  "exclude": [
    "node_modules",
  ]
}
